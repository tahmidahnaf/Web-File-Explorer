style.
  *{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    overflow: hidden;
  }

  .intro-wrapper{
      background: #1F1E1E;
      height: 100vh;
      width: 100vw;
      transition: opacity ease-in-out 0.4s;
      display: flex;
      align-items: center;
      justify-content: center;
      z-index : 100000000000;
      position : absolute;
      top : 0 ;
      left : 0;
  }

  .intro-wrapper #logo{
      position: absolute;
      left: 50%;
      top: 50%;
      transform: translate(-50%,-50%);
      fill: transparent;
      transition: all 0.5s ease-in-out;
      opacity: 0;
  }

  @keyframes letter {
      to{
          stroke-dashoffset: 0;
      }
  }

script.
  let wrapper = document.querySelector('.intro-wrapper')
  let letters = document.querySelectorAll('svg path')
  let word = document.querySelector('svg')
  let all = document.querySelector('*')

  window.addEventListener('load',()=>{
    letters.forEach((l, index) => {
      let delay = index / 2.3
      l.style.strokeDasharray= (l.getTotalLength())+'px'
      l.style.strokeDashoffset = (l.getTotalLength()) + 'px'
      setTimeout(() => {
          word.style.opacity="1"
          l.style.animation = `letter 0.6s ${delay}s ease-in-out forwards`
          if (index === letters.length - 1) {
              setTimeout(() => {
                  word.style.fill = "#F9FAF7"
              }, (delay+0.3) * 1000)
              
              setTimeout(() => {
                  wrapper.style.opacity = "0"
                  all.style.overflowY="scroll"
              }, (delay + 1.2) * 1000)

              setTimeout(() => {
                  wrapper.style.display = "none"
              }, (delay + 1.7) * 1000)
          }
      },300)
    })
  })
